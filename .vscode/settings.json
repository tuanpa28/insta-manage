{
    "conventionalCommits.scopes": [
      "app",
      "components",
      "core",
      "deps",
      "lib",
      "action",
      "structrue",
      "pages"
    ],
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit"
    },
    "editor.formatOnSave": true,
    "css.lint.unknownAtRules": "ignore",
    "scss.lint.unknownAtRules": "ignore"
  }
  